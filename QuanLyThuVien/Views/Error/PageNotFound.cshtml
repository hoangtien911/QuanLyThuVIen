
@{
    ViewBag.Title = "PageNotFound";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Start Bradcaump area -->
<div class="ht__bradcaump__area bg-image--5">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="bradcaump__inner text-center">
                    <h2 class="bradcaump-title">404</h2>
                    <nav class="bradcaump-content">
                        <a class="breadcrumb_item" href="@Url.Action("HomePage", "TrangChu")">Trang chủ</a>
                        <span class="brd-separetor">/</span>
                        <span class="breadcrumb_item active">404</span>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Bradcaump area -->
<!-- Start Error Area -->
<section class="page_error section-padding--lg bg--white">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="error__inner text-center">
                    <div class="error__logo">
                        <a href="#"><img src="~/Content/LibraryImage/images/others/404.png" alt="error images"></a>
                    </div>
                    <div class="error__content">
                        <h2>LỖI - KHÔNG TÌM THẤY TRANG</h2>
                        <p>@(ViewBag.status != null ? ViewBag.status : "")</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End Error Area -->
